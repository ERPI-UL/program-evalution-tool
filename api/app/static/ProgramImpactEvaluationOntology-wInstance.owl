<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/pieo#"
     xml:base="http://purl.org/pieo"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:pieo="http://purl.org/pieo#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://purl.org/pieo">
        <owl:versionIRI rdf:resource="http://purl.org/pieo/1.0"/>
        <dc:description xml:lang="en">This ontology defines a vocabulary that describe the different concept related to program impact evaluation. It is used as basement for creating a digial system that would assist policy-maker when designing policy program and identify the adapted indicators.</dc:description>
        <dc:title xml:lang="en">Ontology for Program Impact Evaluation</dc:title>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/pieo#collect -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#collect">
        <rdfs:domain rdf:resource="http://purl.org/pieo#Indicator"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#DataType"/>
        <rdfs:comment xml:lang="fr">Relation qui lie un indicateur avec un type de données. Cela définie le type de données collecté par l&apos;indicateur.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship that links an indicator with a type of data. This defines the type of data collected by the indicator.</rdfs:comment>
        <rdfs:label xml:lang="en">collect</rdfs:label>
        <rdfs:label xml:lang="fr">collecte</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#defineLogicOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#defineLogicOf">
        <rdfs:domain rdf:resource="http://purl.org/pieo#ResultsChain"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#Program"/>
        <rdfs:comment xml:lang="fr">Relation qui lie une chaine de résultats avec un programme. La chaine de résultats comment les objectifs de développement du programme seront atteints.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship that links a chain of results with a program. The chain of results how the program&apos;s development objectives will be achieved.</rdfs:comment>
        <rdfs:label xml:lang="en">define logic of</rdfs:label>
        <rdfs:label xml:lang="fr">définit le logique de</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#evaluate -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#evaluate">
        <rdfs:domain rdf:resource="http://purl.org/pieo#EvaluationApproach"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#Program"/>
        <rdfs:comment xml:lang="fr">Relation qui lie une démarche d&apos;évaluation avec un programme.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship that links an evaluation approach with a program.</rdfs:comment>
        <rdfs:label xml:lang="en">evaluate</rdfs:label>
        <rdfs:label xml:lang="fr">évalue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#isPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#isPartOf">
        <rdfs:domain rdf:resource="http://purl.org/pieo#stakeholder"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#environment"/>
        <rdfs:comment xml:lang="fr">Relation qui lie une partie prenante avec son environement économique.</rdfs:comment>
        <rdfs:comment xml:lang="en">Relationship that links a stakeholder with its economic environment.</rdfs:comment>
        <rdfs:label xml:lang="fr">fait partie de</rdfs:label>
        <rdfs:label xml:lang="en">is part of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#measure -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#measure">
        <rdfs:label xml:lang="en">measure</rdfs:label>
        <rdfs:label xml:lang="fr">mesure</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#takePlaceIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#takePlaceIn">
        <rdfs:domain rdf:resource="http://purl.org/pieo#Program"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#environment"/>
        <rdfs:comment xml:lang="en">Relation qui lie un programme avec son environement économique.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Relation qui lie un programme avec son environement économique.</rdfs:comment>
        <rdfs:label xml:lang="fr">s&apos;effectue dans</rdfs:label>
        <rdfs:label xml:lang="en">take place in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/pieo#useDataType -->

    <owl:ObjectProperty rdf:about="http://purl.org/pieo#useDataType">
        <rdfs:domain rdf:resource="http://purl.org/pieo#EvaluationApproach"/>
        <rdfs:range rdf:resource="http://purl.org/pieo#DataType"/>
        <rdfs:comment xml:lang="en">A relationship that links an evaluation approach with a type of data. It defines on which type of data an evaluation approach is based.</rdfs:comment>
        <rdfs:comment xml:lang="fr">Relation qui lie une démarche d&apos;évaluation avec un type de données. Ce definie sur quel type de données une démarche d&apos;évaluation se base.</rdfs:comment>
        <rdfs:label xml:lang="en">use data type</rdfs:label>
        <rdfs:label xml:lang="fr">utilise le type de données</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
